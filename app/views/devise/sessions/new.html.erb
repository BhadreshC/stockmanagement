
							<%= link_to "Store", root_path, class: "text-center" %>
							<%= form_for(resource, as: resource_name, url: session_path(resource_name), html: {class: "mt-5 mb-5 login-input"}) do |f| %>
								<div class="form-group"><br />
									<%= f.email_field :email, autofocus: true, autocomplete: "email", placeholder:"Email", class: "form-control" %>
								</div>

								<div class="form-group">
									<%= f.password_field :password, autocomplete: "current-password", placeholder:"password", class: "form-control"%>
								</div>

								<div class="form-group">
									<%= f.submit "Sign In", class: "btn login-form__btn submit w-100" %>
								</div>
							<% end %>
							<%= render "devise/shared/links",class: "mt-5 login-form__footer" %> 
